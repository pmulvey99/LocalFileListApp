<Application x:Class="LocalFileListApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LocalFileListApp"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Run.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/Resources/HardDrive.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/Resources/FolderClosed.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/Resources/Document.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
